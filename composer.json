{
    "name": "dillingham/markdox",
    "description": "Markdown to static content",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "erusev/parsedown": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Markdox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Markdox\\Provider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
